<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
					xmlns:local="clr-namespace:ReceivePlanModule.Presenters"
					xmlns:conv="clr-namespace:WPF.Patterns.Convertors;assembly=PatternLibrary"
					>
	<DataTemplate DataType="{x:Type local:NonExecutedFilterPresenter}">
		<WrapPanel>
			<TextBlock Foreground="Black" Text="{Binding Name}" Margin="5,5,0,5" VerticalAlignment="Center"/>
			<TextBlock Foreground="Black" Grid.Column="1" Text="{Binding FromDateTime, Converter={x:Static conv:ToLocalTime.ToString}}" Margin="5" HorizontalAlignment="Left" VerticalAlignment="Center"/>
		</WrapPanel>
	</DataTemplate>
</ResourceDictionary>