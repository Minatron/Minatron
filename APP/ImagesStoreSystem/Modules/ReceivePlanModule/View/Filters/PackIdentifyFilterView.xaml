<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
					xmlns:local="clr-namespace:ReceivePlanModule.Presenters"
                    xmlns:lang="clr-namespace:LocalizationLibrary;assembly=LocalizationLibrary"
					xmlns:contr="clr-namespace:WPF.Patterns.Controls;assembly=PatternLibrary"
					xmlns:conv="clr-namespace:WPF.Patterns.Convertors;assembly=PatternLibrary"
					>
	<DataTemplate DataType="{x:Type local:PackIdentityFilterPresenter}">
		<Grid>
			<Grid.ColumnDefinitions>
				<ColumnDefinition Width="200"/>
				<ColumnDefinition Width="Auto"/>
			</Grid.ColumnDefinitions>
			<TextBlock Foreground="Black" Text="{Binding Name}" Margin="5" HorizontalAlignment="Left" VerticalAlignment="Center"/>
			<TextBox Grid.Column="1" MinWidth="80" MaxLength="10" Text="{Binding PacketID, UpdateSourceTrigger=PropertyChanged}"/>
		</Grid>
	</DataTemplate>
</ResourceDictionary>